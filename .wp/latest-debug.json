        "WP_DEBUG": true,
        "WP_DEBUG_DISPLAY": true,
        "WP_ENVIRONMENT_TYPE": "local",
        "WP_DEVELOPMENT_MODE": "all",
        "SCRIPT_DEBUG": true
      }
    },
    {
      "step": "login",
      "username": "admin"
    }
  ]
}